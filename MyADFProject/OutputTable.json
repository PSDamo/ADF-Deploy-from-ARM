{

  "name": "OutputTable",
  "properties": {
    "type": "AzureBlob",
    "linkedServiceName": "AzureStorageLinkedService",
    "typeProperties": {
      "folderPath": "adfcontainer/httpDownloadDataIn/{Slice}/",
      "partitionedBy": [
        {
          "name": "Slice",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "yyyyMMddHHmm"
          }
        }
      ]
    },
    "availability": {
      "frequency": "Hour",
      "interval": 1
    }
  }
}